    "@wordpress/babel-preset-default": "^7.29.0",
    "@wordpress/dependency-extraction-webpack-plugin": "^4.29.0",
    "@wordpress/eslint-plugin": "^17.5.0",
    "@wordpress/prettier-config": "^3.3.0",
    "jquery": "^3.7.1",
